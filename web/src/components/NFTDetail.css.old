}
.nft-model {
  margin: 16px 0 10px;
  font-size: 1.6rem;
  font-weight: 600;
}
.spec-list {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 12px;
}
.spec {
  display: flex;
  flex-direction: column;
  gap: 6px;
  padding: 12px;
  border: 1px solid rgba(255, 255, 255, 0.08);
  border-radius: 12px;
}
.spec-label {
  color: #94a3b8;
  font-size: 0.85rem;
}
.spec-value {
  font-family: monospace;
  font-weight: 700;
  color: #e2e8f0;
}
.owner-box h3 {
  margin-top: 0;
}
.owner-address {
  font-family: monospace;
  background: rgba(255, 255, 255, 0.08);
  border: 1px solid var(--glass-border);
  border-radius: 12px;
  padding: 10px 12px;
}
.owner-actions {
  display: flex;
  gap: 10px;
  margin-top: 12px;
}
.btn {
  text-decoration: none;
  padding: 10px 14px;
  border-radius: 10px;
  font-weight: 600;
  cursor: pointer;
}
.btn.primary {
  background: var(--gradient);
  color: #fff;
}
.btn.secondary {
  background: rgba(255, 255, 255, 0.12);
  border: 1px solid var(--glass-border);
  color: #e2e8f0;
}
.btn.warning {
  background: rgba(255, 193, 7, 0.25);
  border: 1px solid rgba(255, 193, 7, 0.45);
  color: #ffda6a;
}
.report-panel {
  margin-top: 16px;
}
.report-item {
  padding: 10px 12px;
  margin-top: 8px;
}
.report-top {
  display: flex;
  justify-content: space-between;
}
.r-status.open {
  color: #ffd166;
}
.r-status.closed {
  color: #06d6a0;
}
.admin-note {
  margin-left: 10px;
  color: #a3e635;
}
.history {
  margin-top: 24px;
  padding: 20px;
}
.timeline {
  display: flex;
  flex-direction: column;
  gap: 12px;
}
.timeline-item {
  display: grid;
  grid-template-columns: 24px 1fr;
  gap: 12px;
}
.dot {
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background: var(--accent2);
  margin-top: 6px;
}
.time-header {
  display: flex;
  gap: 12px;
  color: #94a3b8;
  font-size: 0.85rem;
}
.transfer {
  margin-top: 6px;
}
.tx {
  margin-top: 6px;
  font-family: monospace;
  color: #cbd5e1;
}
.nftdetail-footer {
  display: flex;
  gap: 10px;
  margin-top: 18px;
}
.btn.back {
  background: rgba(255, 255, 255, 0.08);
  border: 1px solid var(--glass-border);
  color: #e2e8f0;
}
.btn.home {
  background: var(--success);
  color: #fff;
}
.modal-overlay {
  position: fixed;
  inset: 0;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  align-items: center;
  justify-content: center;
}
.modal {
  width: 560px;
  max-width: 95vw;
  padding: 20px;
}
.modal textarea {
  width: 100%;
  min-height: 120px;
  margin-top: 10px;
  padding: 10px;
  border-radius: 10px;
  border: 1px solid rgba(255, 255, 255, 0.12);
  background: rgba(255, 255, 255, 0.06);
  color: #e2e8f0;
}
.modal-actions {
  display: flex;
  gap: 10px;
  margin-top: 12px;
}
.report-status {
  margin-top: 10px;
  color: #a3e635;
}
.nftdetail-loading,
.nftdetail-notfound {
  text-align: center;
  padding: 60px;
}
.spinner {
  width: 28px;
  height: 28px;
  border: 3px solid rgba(255, 255, 255, 0.2);
  border-top-color: #06b6d4;
  border-radius: 50%;
  margin: 0 auto 10px;
  animation: spin 1s linear infinite;
}
@keyframes spin {
  to {
    transform: rotate(360deg);
  }
}
@media (max-width: 900px) {
  .nftdetail-grid {
    grid-template-columns: 1fr;
  }
  .history {
    position: static;
    max-height: none;
  }
  .nftdetail-header {
    flex-direction: column;
    gap: 8px;
    align-items: flex-start;
  }
  .nft-badges {
    flex-wrap: wrap;
  }
}
  50% {
    transform: scale(1.4);
    opacity: 0.4;
    gap: 10px;
}

