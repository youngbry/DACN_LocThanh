/* =======================
   üî• WEB3 DARK PRO STYLE 
   ======================= */

:root {
  --bg1: #0f172a;
  --bg2: #1e293b;
  --glass-bg: rgba(255, 255, 255, 0.06);
  --glass-border: rgba(255, 255, 255, 0.12);

  --accent1: #6366f1;
  --accent2: #06b6d4;
  --success: #10b981;

  --gradient: linear-gradient(135deg, var(--accent1), var(--accent2));
}

/* MAIN PAGE */
.my-nfts-container {
  min-height: 100vh;
  padding: 40px 20px;
  font-family: "Segoe UI", sans-serif;
  color: #e2e8f0;
  position: relative;
}

/* BACKGROUND EFFECTS */
.my-nfts-container::before,
.my-nfts-container::after {
  content: "";
  position: fixed;
  inset: 0;
  z-index: -2;
}

.my-nfts-container::before {
  background: radial-gradient(circle at 20% 30%, #1e3a8a33, transparent 60%),
    radial-gradient(circle at 85% 70%, #0d948833, transparent 65%),
    linear-gradient(120deg, var(--bg1), var(--bg2));
}

.my-nfts-container::after {
  background: repeating-linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.05) 0px,
    rgba(255, 255, 255, 0.05) 2px,
    transparent 2px,
    transparent 8px
  );
  opacity: 0.3;
  animation: gridMove 25s linear infinite;
}

@keyframes gridMove {
  from {
    transform: translateY(0);
  }
  to {
    transform: translateY(30px);
  }
}

/* ===========================
   üî• HEADER
   =========================== */
.mynft-header {
  background: var(--glass-bg);
  border: 1px solid var(--glass-border);
  backdrop-filter: blur(14px);
  padding: 32px 40px;
  border-radius: 24px;
  box-shadow: 0 12px 35px -10px rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  gap: 20px;
  margin-bottom: 40px;
  position: relative;
}

.mynft-header h1 {
  margin: 0;
  font-size: 2.3rem;
  font-weight: 700;
  background: var(--gradient);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.mynft-header p {
  margin: 5px 0 0 0;
  color: #94a3b8;
  font-size: 1rem;
}

.mynft-wallet {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.wallet-label {
  text-transform: uppercase;
  font-size: 0.75rem;
  color: #94a3b8;
  font-weight: 600;
}

.wallet-address {
  font-family: monospace;
  padding: 8px 14px;
  background: rgba(255, 255, 255, 0.12);
  border: 1px solid var(--glass-border);
  border-radius: 12px;
  margin-top: 5px;
}

.wallet-status {
  display: flex;
  align-items: center;
  gap: 8px;
  font-size: 0.8rem;
  color: var(--success);
  font-weight: bold;
  margin-top: 6px;
}

.wallet-status span {
  width: 8px;
  height: 8px;
  background: var(--success);
  border-radius: 50%;
  animation: pulseDot 2s ease-in-out infinite;
}

@keyframes pulseDot {
  0%,
  100% {
    transform: scale(1);
    opacity: 1;
  }
  50% {
    transform: scale(1.4);
    opacity: 0.4;
  }
}

/* ================================
   üèçÔ∏è NFT GRID
   ================================= */
.nfts-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(330px, 1fr));
  gap: 30px;
}

/* NFT CARD */
.nft-card {
  background: var(--glass-bg);
  border: 1px solid var(--glass-border);
  border-radius: 24px;
  backdrop-filter: blur(18px) saturate(160%);
  box-shadow: 0 15px 40px -12px rgba(0, 0, 0, 0.55);
  overflow: hidden;
  transition: 0.4s ease;
}

.nft-card:hover {
  transform: translateY(-6px);
  box-shadow: 0 25px 55px -15px rgba(0, 0, 0, 0.75);
}

/* HEADER BAR */
.nft-card-banner {
  height: 190px;
  background: var(--gradient);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 4rem;
  color: white;
}

.nft-card-content {
  padding: 26px;
}

.nft-card-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.nft-id,
.nft-year {
  padding: 6px 12px;
  background: rgba(255, 255, 255, 0.18);
  border-radius: 8px;
  font-weight: bold;
  font-size: 0.8rem;
}

.nft-locked {
  padding: 6px 12px;
  background: rgba(255, 0, 0, 0.25);
  border-radius: 8px;
  font-size: 0.75rem;
  font-weight: bold;
  color: #ff6b6b;
}

/* MODEL */
.nft-title {
  margin: 18px 0 15px;
  font-size: 1.4rem;
  font-weight: 600;
  color: #fff;
}

/* DETAILS */
.detail-row {
  display: flex;
  justify-content: space-between;
  padding: 8px 0;
  font-size: 0.85rem;
  border-bottom: 1px solid rgba(255, 255, 255, 0.08);
}

.detail-row:last-child {
  border-bottom: none;
}

.label {
  color: #94a3b8;
}

.value {
  font-family: monospace;
  font-weight: bold;
  color: #e2e8f0;
}

/* ACTION BUTTONS */
.nft-actions {
  display: flex;
  gap: 12px;
  margin-top: 20px;
}

.nft-btn {
  flex: 1;
  text-align: center;
  padding: 12px 14px;
  border-radius: 12px;
  font-weight: 600;
  font-size: 0.9rem;
  cursor: pointer;
  text-decoration: none;
  transition: 0.35s ease;
}

/* Primary */
.nft-btn.primary {
  background: var(--gradient);
  color: white;
}

.nft-btn.primary:hover {
  transform: translateY(-3px);
  box-shadow: 0 12px 30px rgba(6, 182, 212, 0.45);
}

/* Secondary */
.nft-btn.secondary {
  background: rgba(255, 255, 255, 0.12);
  border: 1px solid var(--glass-border);
  color: #e2e8f0;
}

.nft-btn.secondary:hover {
  transform: translateY(-3px);
  background: rgba(255, 255, 255, 0.18);
}

/* Disabled */
.nft-btn.disabled {
  background: rgba(255, 255, 255, 0.08);
  color: #888;
  cursor: not-allowed;
}

/* ===============================
   NO NFT SECTION
   =============================== */
.no-nfts {
  text-align: center;
  padding: 60px;
  background: var(--glass-bg);
  border-radius: 24px;
  border: 1px solid var(--glass-border);
}

.no-nfts-icon {
  font-size: 4rem;
  opacity: 0.6;
  margin-bottom: 20px;
}

.no-nfts h3 {
  font-size: 1.8rem;
  color: #fff;
}

.no-nfts p {
  color: #94a3b8;
}

/* FOOTER */
.mynft-footer {
  text-align: center;
  margin-top: 30px;
}

.refresh-btn {
  background: var(--success);
  padding: 12px 22px;
  border: none;
  border-radius: 20px;
  color: white;
  font-weight: bold;
  cursor: pointer;
  transition: 0.3s ease;
}

.refresh-btn:hover {
  transform: translateY(-3px);
  background: #0d9f73;
}

/* RESPONSIVE */
@media (max-width: 768px) {
  .mynft-header {
    text-align: center;
    flex-direction: column;
    align-items: center;
  }
  .mynft-wallet {
    align-items: center;
  }
}

